pipeline {
    agent any
    environment {
        PATH = "/opt/maven/bin:$PATH"
    }
    stages {
        stage('Git') {
            steps {
                git branch: 'main', changelog: false, poll: false, url: 'https://github.com/rajuhpr/DevOps-demo.git'
                echo "Git clone is success! "
            }
        }
        stage('Build') {
            steps {
                sh "cd jenkin-ans-k8s && mvn clean install"
            }
        }        
    }
}



